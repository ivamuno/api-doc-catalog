{
  "name": "internal-api-explorer",
  "version": "0.3.1",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && copyfiles docs/**/* build && npm run pack:postman",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "npm run build && npm run build:apis && light-server -s build",
    "pack:postman": "mkdirp public/assets/postman && cd src/assets/postman/ && bestzip ../../../public/assets/postman/internal-api-docs.postman.zip *",
    "build:apis": "npm run build:identity",
    "build:cards-tokenization": "npm run build:cards-tokenization-api && npm run build:cards-tokenization-crypto-api",
    "build:cards-tokenization-api": "redocly bundle apis/cards-tokenization/api/openapi.yaml -o build/docs/cards-tokenization-api.yaml",
    "build:cards-tokenization-crypto-api": "redocly bundle apis/cards-tokenization/crypto-api/openapi.yaml -o build/docs/cards-tokenization-crypto-api.yaml",
    "build:front-office-config": "redocly bundle apis/front-office-config/openapi.yaml -o build/docs/front-office-config-api.yaml",
    "build:identity": "redocly bundle apis/identity/openapi.yaml -o build/docs/identity-api.yaml",
    "build:sensitive-data": "redocly bundle apis/sensitive-data/openapi.yaml -o build/docs/sensitive-data-api.yaml"
  },
  "dependencies": {
    "codemirror": "^5.59.2",
    "core-js": "^3.1.4",
    "mobx": "^6.0.4",
    "react": "^16.13.1",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-intl": "^4.5.2",
    "react-markdown": "^5.0.3",
    "react-pro-sidebar": "^0.4.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-split-pane": "^0.1.92",
    "react-switch": "^5.0.1",
    "redoc-asyncapi": "^2.0.0",
    "styled-components": "^4.1.1 || ^5.1.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.0.0-beta.107",
    "bestzip": "^2.2.1",
    "copyfiles": "^2.4.1",
    "light-server": "^2.9.1",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "tslint": "5.16.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
